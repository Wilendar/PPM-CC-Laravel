# Dependencies
/vendor/
/node_modules/

# Environment
/.env
/.env.*
!/.env.example

# Build Artifacts
/public/build/
/public/hot
/public/storage

# Bootstrap Cache
/bootstrap/cache/*.php

# Storage (runtime data)
/storage/app/containers/
/storage/app/products/images/
/storage/app/imports/
/storage/app/exports/
/storage/logs/*.log
/storage/*.key
/storage/backups/

